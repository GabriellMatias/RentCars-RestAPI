        "requestBody":{
          "content":{
            "application/json":{
              "schema":{
                "type": "object",
                "properties":{
                  "name":{
                    "type":"string"
                  },
                  "description":{
                     "type":"string"
                  }

                },
                "example":{
                  "name":"category name",
                  "description":"category desc"
                }
              }
            }
          }
        },
        "responses":{
          "201":{
            "description":"created"
          },
          "500":{
            "description":"Category already exists"
          }
          
        }
      },
      "get":{
        "tags":["Category"],
        "summary":"list all categories",
        "description":"Listing all categories",
        "responses":{
          "200":{
            "description":"list sucessfully",
            "content":{"application/json":{
              "schema":{
                "type":"array",
                "items":{
